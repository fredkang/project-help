<!DOCTYPE html>
<html>

<!-- start Mixpanel -->
<%= render "layouts/mixpanel" %>
<!-- end Mixpanel -->

<script>
	mixpanel.track_links("#add-group-link", "Add-group Link Clicked", {
        referrer: document.referrer
    });
</script>

<head>
  <title>ProjectHelp</title>
  <%= stylesheet_link_tag    "application", "//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css", "//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap-theme.min.css", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "//netdna.bootstrapcdn.com/bootstrap/3.0.0/js/bootstrap.min.js", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<%= render "layouts/navbar" %>

	<div class="col-sm-10 col-sm-offset-1">
		<%= yield %>
	</div>
</body>