<!DOCTYPE html>
<html>

<!-- start Mixpanel -->
<%= render "layouts/mixpanel" %>
<!-- end Mixpanel -->

<head>
  <title>Project Help</title>
  <%= stylesheet_link_tag    "application", "//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	
	<!-- Navbar with only the hompage link -->
	<div class = "navbar navbar-inverse navbar-fixed-top">
		<div class="container">
			<div class="navbar-header">
				<a href="/users"><span class="navbar-brand">Project Help <small>alpha</small></span></a>
			</div>

		</div>
	</div>
	
	<div class="col-sm-10 col-sm-offset-1">
		<%= yield %>
	</div>
</body>